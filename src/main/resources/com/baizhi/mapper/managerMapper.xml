<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.cmfz.dao.ManagerDao">

    <insert id="insert">
        INSERT INTO manager VALUES (#{id}, #{name}, #{password}, #{phone})
    </insert>

    <select id="queryByPhone" resultType="com.baizhi.cmfz.entity.Manager">
        SELECT
            id,
            name,
            password,
            phone
        FROM manager
        WHERE phone = #{phone}
    </select>

</mapper>